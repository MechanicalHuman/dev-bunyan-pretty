{
  "extends": "hnp",
  "rules": {
    "lodash-fp/use-fp": "warn",
    "lodash-fp/no-chain": "warn",
    "lodash-fp/consistent-name": "off",
    "header/header": [
      2,
      "block",
      [
        "!",
        {
          "pattern": " \\* Copyright \\d{4} Jorge Proaño.",
          "template": " * Copyright 2019 Jorge Proaño."
        },
        " "
      ]
    ]
  }
}
