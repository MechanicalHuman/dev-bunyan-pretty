{
  "name": "@mechanicalhuman/bunyan-pretty",
  "version": "2.0.8",
  "description": "Prettifies NDJSON (Newline Delimited JSON) logs, like `bunyan -o short` but actually pretty.",
  "keywords": [
    "bunyan",
    "pino"
  ],
  "homepage": "https://github.com/MechanicalHuman/dev-bunyan-pretty#readme",
  "bugs": {
    "url": "https://github.com/MechanicalHuman/dev-bunyan-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MechanicalHuman/dev-bunyan-pretty.git"
  },
  "license": "MIT",
  "author": "Jorge Proaño <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)",
  "contributors": [
    "Jorge Proaño <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)"
  ],
  "main": "lib/index.js",
  "bin": {
    "pretty": "./bin/pretty.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "precommit": "npm run docs && lint-staged",
    "docs": "run-s docs:** -sn",
    "postdocs": "git add README.md AUTHORS LICENSE package.json",
    "docs:authors": "git log --format='%aN <%aE>' | sort -f | uniq > AUTHORS",
    "docs:compile": "projectz compile",
    "prepush": "npm run test",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky,config --ignore-bin-package",
    "test:lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test:outdated": "npm outdated --long || true",
    "todos": "leasot './**/*' -Sx -i './node_modules/**/*'"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "config": "^2.0.1",
    "debug": "^3.1.0",
    "ipaddr.js": "^1.8.1",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.21",
    "parse-headers": "^2.0.1",
    "pretty-ms": "^3.2.0",
    "split2": "^2.2.0",
    "string-length": "^2.0.0",
    "supports-color": "^5.4.0",
    "term-size": "^1.2.0",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "eslint": "^5.2.0",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "leasot": "^6.3.0",
    "lint-staged": "^7.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.0",
    "projectz": "^1.4.0",
    "sort-package-json": "^1.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "maintainers": []
}
