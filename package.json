{
  "name": "@mechanicalhuman/bunyan-pretty",
  "version": "2.0.8",
  "description": "Prettifies NDJSON (Newline Delimited JSON) logs, like `bunyan -o short` but actually pretty.",
  "keywords": [
    "bunyan",
    "pino"
  ],
  "homepage": "https://github.com/MechanicalHuman/dev-bunyan-pretty#readme",
  "bugs": {
    "url": "https://github.com/MechanicalHuman/dev-bunyan-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MechanicalHuman/dev-bunyan-pretty.git"
  },
  "license": "MIT",
  "author": "Jorge Proaño <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)",
  "contributors": [
    "Jorge Proaño <jorge@hiddennodeproblem.com>"
  ],
  "main": "lib/index.js",
  "bin": {
    "pretty": "./bin/pretty.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "docs": "package-scripts compile",
    "lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test": "run-s test:* lint -sn",
    "test:depcheck": "depcheck --ignores=husky,config --ignore-bin-package",
    "test:outdated": "npm outdated --long || true",
    "todos": "package-scripts todos"
  },
  "husky": {
    "hooks": {
      "pre-commit": "package-scripts compile; lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@hnp/cz"
    }
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "config": "^3.0.1",
    "debug": "^4.1.1",
    "ipaddr.js": "^1.8.1",
    "lodash": "^4.17.11",
    "moment-timezone": "^0.5.23",
    "parse-headers": "^2.0.1",
    "pretty-ms": "^4.0.0",
    "split2": "^3.1.0",
    "string-length": "^2.0.0",
    "supports-color": "^6.1.0",
    "term-size": "^1.2.0",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@hnp/cz": "^1.0.7",
    "@hnp/package-scripts": "^1.0.10",
    "depcheck": "^0.7.1",
    "eslint": "^5.12.1",
    "eslint-config-hnp": "^1.0.7",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
