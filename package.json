{
  "name": "@mechanicalhuman/bunyan-pretty",
  "version": "2.1.1",
  "description": "Prettifies NDJSON (Newline Delimited JSON) logs, like `bunyan -o short` but actually pretty.",
  "keywords": [
    "bunyan",
    "pino"
  ],
  "homepage": "https://github.com/MechanicalHuman/dev-bunyan-pretty#readme",
  "bugs": {
    "url": "https://github.com/MechanicalHuman/dev-bunyan-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MechanicalHuman/dev-bunyan-pretty.git"
  },
  "license": "MIT",
  "author": "Jorge Proaño <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)",
  "contributors": [
    "Jorge Proaño <jorge@hiddennodeproblem.com>"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": {
    "pretty": "bin/pretty"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test": "echo \"Error: No tests defined\" && exit 0",
    "todos": "package-scripts todos"
  },
  "husky": {
    "hooks": {
      "pre-commit": "package-scripts compile; lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@hnp/cz"
    }
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "debug": "^4.1.1",
    "ipaddr.js": "^1.8.1",
    "lodash": "^4.17.11",
    "moment-timezone": "^0.5.23",
    "parse-headers": "^2.0.1",
    "pretty-ms": "^4.0.0",
    "pump": "^3.0.0",
    "split2": "^3.1.0",
    "string-length": "^2.0.0",
    "term-size": "^1.2.0",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@hnp/cz": "^1.0.7",
    "@hnp/package-scripts": "^1.0.11",
    "eslint": "^5.13.0",
    "eslint-config-hnp": "^1.0.8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
