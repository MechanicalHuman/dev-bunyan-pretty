{
  "name": "@mechanicalhuman/bunyan-pretty",
  "version": "2.0.8",
  "description": "Prettifies NDJSON (Newline Delimited JSON) logs, like `bunyan -o short` but actually pretty.",
  "keywords": [
    "bunyan",
    "pino"
  ],
  "homepage": "https://github.com/MechanicalHuman/dev-bunyan-pretty#readme",
  "bugs": {
    "url": "https://github.com/MechanicalHuman/dev-bunyan-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:MechanicalHuman/dev-bunyan-pretty.git"
  },
  "license": "MIT",
  "author": "Jorge Proaño <jorge@hiddennodeproblem.com> (http://www.hidden-node-problem.com)",
  "contributors": [
    "Jorge Proaño <jorge@hiddennodeproblem.com>"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "precompile": "rm -rf dist/*",
    "compile": "tsc",
    "dev": "npm run compile -- --watch",
    "lint": "tslint -p tsconfig.json",
    "prepublishOnly": "npm run compile",
    "test": "echo \"Error: No tests defined\" && exit 1",
    "todos": "package-scripts todos"
  },
  "husky": {
    "hooks": {
      "pre-commit": "package-scripts compile; lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,js}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@hnp/cz"
    }
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "config": "^3.0.1",
    "debug": "^4.1.1",
    "ipaddr.js": "^1.8.1",
    "lodash": "^4.17.11",
    "moment-timezone": "^0.5.23",
    "parse-headers": "^2.0.1",
    "pretty-ms": "^4.0.0",
    "split2": "^3.1.0",
    "string-length": "^2.0.0",
    "supports-color": "^6.1.0",
    "term-size": "^1.2.0",
    "update-notifier": "^2.5.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@hnp/cz": "^1.0.7",
    "@hnp/package-scripts": "^1.0.10",
    "@types/debug": "0.0.31",
    "@types/lodash": "^4.14.120",
    "@types/update-notifier": "^2.5.0",
    "@types/yargs": "^12.0.5",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "tslint": "^5.12.1",
    "tslint-config-hnp": "^1.2.1",
    "typescript": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
