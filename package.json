{
  "name": "@mechanicalhuman/bunyan-pretty",
  "version": "2.0.0",
  "description": "Pretty format for Bunyan/Pino logs, similar to `bunyan -o short` but actually pretty. ",
  "keywords": [
    "bunyan",
    "pino"
  ],
  "homepage": "https://github.com/MechanicalHuman/dev-bunyan-pretty#readme",
  "bugs": {
    "url": "https://github.com/MechanicalHuman/dev-bunyan-pretty/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MechanicalHuman/dev-bunyan-pretty.git"
  },
  "license": "MIT",
  "author": "Jorge Proa√±o <jorge@hiddennodeproblem.com> (https://www.hidden-node-problem.com)",
  "main": "lib/index.js",
  "bin": {
    "pretty": "./bin/pretty.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "precommit": "npm run docs && lint-staged",
    "docs": "run-s docs:* -sn  && git add {README,TODO}.md",
    "docs:readme": "mos README.md",
    "docs:todos": "npm run todos -s -- --reporter vscode > TODO.md",
    "prepush": "npm run test",
    "test": "run-s test:* -sn",
    "test:depcheck": "depcheck --ignores=husky,config --ignore-bin-package",
    "test:lint": "eslint --ignore-path .gitignore '**/*.js'",
    "test:outdated": "npm outdated --long",
    "todos": "leasot './**/*' -Sx -i './node_modules/**/*'"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "debug": "^3.0.1",
    "ipaddr.js": "^1.7.0",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.17",
    "parse-headers": "^2.0.1",
    "pretty-ms": "^3.2.0",
    "split2": "^2.1.1",
    "string-length": "^2.0.0",
    "supports-color": "^5.4.0",
    "term-size": "^1.2.0",
    "update-notifier": "^2.5.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.9",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "leasot": "^6.3.0",
    "lint-staged": "^7.1.0",
    "mos": "^1.3.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.9.2",
    "sort-package-json": "^1.15.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
